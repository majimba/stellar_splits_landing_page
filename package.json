{
  "name": "stellar-splits-landing-page",
  "version": "1.0.0",
  "description": "Landing page for Stellar Splits - African music split management platform",
  "main": "index.html",
  "dependencies": {
    "@dhiwise/component-tagger": "^1.0.9",
    "@tailwindcss/forms": "^0.5.7",
    "tailwindcss-animate": "^1.0.7",
    "tailwindcss-elevation": "^2.0.0",
    "tailwindcss-fluid-type": "^2.0.7"
  },
  "scripts": {
    "build": "npm run build:css",
    "build:css": "npx @dhiwise/component-tagger && npx tailwindcss -i ./css/tailwind.css -o ./css/main.css --minify",
    "watch:css": "npx @dhiwise/component-tagger && npx tailwindcss -i ./css/tailwind.css -o ./css/main.css --watch",
    "dev": "npm run watch:css"
  },
  "keywords": ["stellar-splits", "music", "splits", "africa", "landing-page", "tailwindcss"],
  "author": "Arc Synthesis",
  "license": "MIT",
  "devDependencies": {
    "tailwindcss": "^3.4.17",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/line-clamp": "^0.1.0",
    "@tailwindcss/typography": "^0.5.16"
  }
}